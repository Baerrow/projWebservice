<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8" />
        <title>{% block title %}APPLICATION DE SAGA AUDIOS{% endblock %}</title>
        {% block stylesheets %}
        <link href="{{ asset('css/bootstrap.css') }}" type="text/css" rel="stylesheet" />
        <link href="{{ asset('css/style.css') }}" type="text/css" rel="stylesheet" />{% endblock %}
        <link rel="icon" type="image/x-icon" href="{{ asset('sausage.png') }}" />
        <!-- dl jquery -->
        <script type="text/javascript" src="{{ asset('js/jquery-1.11.3.min.js') }}"></script>
        <script type="text/javacript">
            $(document).ready(function() {$('datatable').DataTable();} ); 
        </script>
        
    </head>
    <body>
        
        <ul class="nav nav-tabs">
          <li><a href="{{ path('audios') }}" class="active">Audios</a></li>
          <li><a href="{{ path('playlists') }}" class="active">Playlists</a></li>
          <li><a href="{{ path('authors') }}" class="active">Auteurs</a></li>
          <li><a href="{{ path('genres') }}" class="active">Genres</a></li>
        </ul>

        <div class="container">
        {% block body %}{% endblock %}
        </div>
        {% block javascripts %}{% endblock %}
    </body>
</html>
